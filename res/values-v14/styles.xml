<resources>

    <!--
        Base application theme for API 14+. This theme completely replaces
        AppBaseTheme from BOTH res/values/styles.xml and
        res/values-v11/styles.xml on API 14+ devices.
    -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- API 14 theme customizations can go here. -->
        
          <item name="colorPrimary">@color/abc_input_method_navigation_guard</item>
        <item name="colorPrimaryDark">@color/switch_thumb_normal_material_dark</item>
        <item name="colorAccent">@color/abc_primary_text_disable_only_material_dark</item>
    </style>

</resources>
